;
; FUNCTION NUMBER DEFINITIONS FOR SYSTEM FUNCTIONS IN LCD MODULE:
;
LCD_INIT       EQU     0               ; INIT_LCD
LCD_WAIT       EQU     2               ; LCDWAIT
WR_SCRN        EQU     4               ; WRITE_SCREEN
WR_PRM_VAL     EQU     6               ; WRITE_PARAM_VALUE
WR_VAL         EQU     8               ; WRITE_VALUE
WR_PRM_STRNG   EQU     10              ; WRITE_PARAM_STRING
WR_A0_STRNG    EQU     12              ; WRITE_A0_STRING
WR_STRNG       EQU     14              ; WRITE_STRING
DSP_SCRN       EQU     16              ; DISP_SCREEN
DSP_STRNG      EQU     18              ; DISP_STRING
DSP_BUF        EQU     20              ; DISP_BUFFER
DSP_PRM_FLD    EQU     22              ; DISP_PARAM_FIELD
DSP_FLD        EQU     24              ; DISP_FIELD
DSP_HEX        EQU     26              ; DISP_HEX
LNG_BIN_2_BCD  EQU     28              ; LONG_BIN_TO_BCD
WRD_BIN_2_BCD  EQU     30              ; WORD_BIN_TO_BCD
INS_DCML_PT    EQU     32              ; INSERT_DECIMAL_PT
BLNK_ZEROS     EQU     34              ; BLANK_ZEROES
WR_BCD_DGTS    EQU     36              ; WRITE_BCD_DIGITS
SET_SGN_CHR    EQU     38              ; SET_SIGN_CHAR
WR_SGN_CHR     EQU     40              ; WRITE_SIGN_CHAR
UPD_CRSR       EQU     42              ; UPDATE_CURSOR
STO_GRPHCS     EQU     44              ; STORE_GRAPHICS
WR_HEX_PRM     EQU     46              ; WRITE_HEX_PARAM
WR_HEX_VAL     EQU     48              ; WRITE_HEX_VALUE
WR_ENCD_SCRN   EQU     50              ; WRITE_ENCODED_SCREEN
DSP_ENCD_SCRN  EQU     52              ; DISP_ENCODED_SCREEN
;
