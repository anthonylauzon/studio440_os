               INCLUDE HPFIXUPS
               TITLE "SEQUENCE TIMING PHASE TABLES, ETC." 
*************************************************************************************************** 
*************************************************************************************************** 
***                                                                                             *** 
***            TABLES AND TABLES OF TABLES OF PHASES WE GO THROUGH IN RECORD MODE               *** 
***                                                                                             *** 
*************************************************************************************************** 
*************************************************************************************************** 
; 
               ABS_SHORT
; 
               GLB     PHASE_LENGTHS,PHASE_TABLES_TBL,SWING_PER_TBL 
; 
               GLB     FAZE_16_S0      ;SAFETY DEFAULT PHASE_LIST INITIAL VALUE (RESET_440).
; 
               EXTERNAL PHASE_0,PHASE_1,PHASE_2,PHASE_3,PHASE_4 
; 
; 
; 
;VVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVV 
; 
PHASE_LENGTHS          ; THE NUMBER OF STAGES PER AUTOCORRECT VALUE.  USUALLY THE NUMBER OF CLICKS
; PER AUTOCORRECT INTERVAL * 2 (TO TAKE INTO ACCOUNT SWING ON THE EVEN NUMBERED INTERVALS). 
               DC.W    1               ;OFF 
               DC.W    4               ;32 TRIPLET
               DC.W    6               ;32
               DC.W    8               ;16 TRIPLET
               DC.W    12              ;16
               DC.W    16              ;8 TRIPLET 
               DC.W    24              ;8 
               DC.W    32              ;4 TRIPLET 
               DC.W    48              ;4 
  
  
PHASE_TABLES_TBL       ; FOR EACH AUTOCORRECT VALUE, THERE IS A TABLE THAT NEEDS TO BE REFERENCED 
; THAT CONTAINS FURTHER LABELS OF PHASE TABLES (1 PER SWING VALUE PER AUTOCORRECT INTERVAL) - BUT 
; WE'RE GETTING AHEAD OF OURSELVES HERE.  THIS IS THE FIRST LINE, PER AUTOCORRECT INTERVAL. 
               DC.L    AUTO_OFF        ;OFF 
               DC.L    AUTO_32T        ;32 TRIPLET
               DC.L    AUTO_32         ;32
               DC.L    AUTO_16T        ;16 TRIPLET
               DC.L    AUTO_16         ;16
               DC.L    AUTO_8T         ;8 TRIPLET 
               DC.L    AUTO_8          ;8 
               DC.L    AUTO_4T         ;4 TRIPLET 
               DC.L    AUTO_4          ;4 
  
  
SWING_PER_TBL          ; TABLE PER AUTOCORRECT INTERVAL FOR SWING PERCENTAGES PER SWING VALUE 
               DC.L    SW_OFF 
               DC.L    SW_32T 
               DC.L    SW_32
               DC.L    SW_16T 
               DC.L    SW_16
               DC.L    SW_8T
               DC.L    SW_8 
               DC.L    SW_4T
               DC.L    SW_4 
; 
;AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 
               SKIP 
;VVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVVV 
; 
;             BELOW ARE (BIG BREATH NOW) TABLES (PER AUTOCORRECT INTERVAL) OF THE PHASE TABLE TO
;              USE, PER SWING POSSIBILITY.
; 
;             LET ME PUT IT ANOTHER WAY - FOR EVERY POSSIBLE COMBINATION OF AUTOCORRECT AND SWING,
;              THERE NEEDS TO BE A PHASE TABLE TELLING THE RECORDING MECHANISM WHAT TO DO EVERY CLICK.
; 
AUTO_OFF
               DC.L    FAZE_OFF_S0
  
AUTO_32T
               DC.L    FAZE_32T_S0
               DC.L    FAZE_32T_S1
  
AUTO_32 
               DC.L    FAZE_32_S0 
               DC.L    FAZE_32_S1 
  
AUTO_16T
               DC.L    FAZE_16T_S0
               DC.L    FAZE_16T_S1
               DC.L    FAZE_16T_S2
  
AUTO_16 
               DC.L    FAZE_16_S0 
               DC.L    FAZE_16_S1 
               DC.L    FAZE_16_S2 
               DC.L    FAZE_16_S3 
  
AUTO_8T 
               DC.L    FAZE_8T_S0 
               DC.L    FAZE_8T_S1 
               DC.L    FAZE_8T_S2 
               DC.L    FAZE_8T_S3 
               DC.L    FAZE_8T_S4 
  
AUTO_8
               DC.L    FAZE_8_S0
               DC.L    FAZE_8_S1
               DC.L    FAZE_8_S2
               DC.L    FAZE_8_S3
               DC.L    FAZE_8_S4
               DC.L    FAZE_8_S5
